{
  "version": 2,
  "builds": [
    {
      "src": "app/api.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "pythonVersion": "3.12",
        "installCommand": "pip install --no-cache-dir -r requirements-prod.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/api.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  }
}
